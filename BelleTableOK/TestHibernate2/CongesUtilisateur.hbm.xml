<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
namespace="PamNHibernateDemos" assembly="TestHibernate2">
  <class name="CongesUtilisateur" table="conges_utilisateur">
    <id name="idCongesUtilisateur" column="idCongesUtilisateur" unsaved-value="0">
      <generator class="native" />
    </id>
    <bag name="lesUtilisateurs">
      <key column="numUtilsateur" />
      <one-to-many class="Utilisateur"/>
    </bag>
    <bag name="lesConges">
      <key column="numConge" />
      <one-to-many class="Conge"/>
    </bag>
  </class>
</hibernate-mapping>
