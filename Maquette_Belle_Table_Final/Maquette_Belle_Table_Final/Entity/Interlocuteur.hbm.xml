<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
namespace="Maquette_Belle_Table_Final" assembly="Maquette_Belle_Table_Final">
  <class name="Interlocuteur" table="interlocuteur">
    <id name="idInterlocuteur" column="idInterlocuteur" unsaved-value="0">
      <generator class="native" />
    </id>

    <property name="nomInterlocuteur" column="nomInterlocuteur"/>
    <property name="prenomInterlocuteur" column="prenomInterlocuteur"/>
    <property name="telInterlocuteur" column="telInterlocuteur"/>
    <property name="mailInterlocuteur" column="mailInterlocuteur"/>
    
    <one-to-one name="portefeuille" constrained="true" foreign-key="none" class="PorteFeuille" lazy="false"/>

    <set name="lesRendezVous">
      <key column="idRdv" />
      <one-to-many class="RendezVous"/>
    </set>

    <set name="lesMails">
      <key column="idInterlocuteur" />
      <one-to-many class="Mail"/>
    </set>
    
    <joined-subclass table="individu" name="Individu">
		<key column="idInterlocuteur"/>
    <property name="distanceSiege" column="distanceSiege"/>
    <property name="adresseIndividu" column="adresseIndividu"/>
    <property name="cpIndividu" column="cpIndividu"/>
    <property name="villeIndividu" column="villeIndividu"/>
    <property name="planAcces" column="planAcces"/>
    <property name="infosSupplementaire" column="infosSupplementaire"/>
	  </joined-subclass>
    
  </class>
</hibernate-mapping>
