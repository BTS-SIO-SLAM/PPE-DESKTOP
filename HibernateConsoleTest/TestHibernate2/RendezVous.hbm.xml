<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
namespace="PamNHibernateDemos" assembly="TestHibernate2">
  <class name="RendezVous" table="rdv">
    <id name="idRdv" column="id_rdv" unsaved-value="0">
      <generator class="native" />
    </id>
    <property name="codeEntreeDerogatoire" column="codeEntreeDerogatoire"/>
    <property name="dateRdv" column="date_rdv"/>
    <property name="heureDebut" column="heure_debut"/>
    <property name="heureFin" column="heure_fin"/>
    <property name="adresseDerogatoire" column="adresseDerogatoire"/>
    <property name="villeDerogatoire" column="villeDerogatoire"/>
    <property name="infoDerogatoire" column="infoDerogatoire"/>
    <many-to-one name="interlocuteur" column="id_interlocuteur" cascade="all" lazy="false"/>
    <many-to-one name="typeRdv" column="id_type_rdv" cascade="all" lazy="false"/>
    <many-to-one name="planning" column="idPlanning" cascade="all" lazy="false"/>
  </class>
</hibernate-mapping>