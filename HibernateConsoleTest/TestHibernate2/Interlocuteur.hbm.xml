<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
namespace="PamNHibernateDemos" assembly="TestHibernate2">
  <class name="Interlocuteur" table="interlocuteur">
    <id name="idInterlocuteur" column="id_interlocuteur" unsaved-value="0">
      <generator class="native" />
    </id>

    <property name="nomInterlocuteur" column="nom_interlocuteur"/>
    <property name="prenomInterlocuteur" column="prenom_interlocuteur"/>
    <property name="telInterlocuteur" column="tel_interlocuteur"/>
    <property name="mailInterlocuteur" column="mail_interlocuteur"/>
    <many-to-one name="porteFeuille" column="idPortefeuille" cascade="all" lazy="false"/>
    <one-to-one name="individu" constrained="true" foreign-key="none" class="Individu" />
  </class>
</hibernate-mapping>
